class Solution {
    public int maxProfit(int[] prices) {
        int min = prices[0];
        int mP = Integer.MIN_VALUE;
        for(int i=1;i<prices.length;i++){
            mP = Math.max(prices[i] - min, mP);
            min = Math.min(prices[i], min);
        }
        if(mP<0){
            return 0;
        }else{
            return mP;
        }
    }
}
