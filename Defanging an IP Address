class Solution {
    public String defangIPaddr(String address) {
        StringBuilder re = new StringBuilder();
        int n = address.length();
        for(int i=0;i<n;i++){
            if(address.charAt(i)=='.'){
                re.append("[.]");
            }else{
                re.append(address.charAt(i));
            }
        }
        return re.toString();
e
