class Solution {
    public int maxProduct(int[] nums) {
        Arrays.sort(nums);
        int n = nums.length;
        int max = Integer.MIN_VALUE;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                int re = (nums[i]-1)*(nums[j]-1);
                max = Math.max(re, max);
            }
        }
        return max;
    }
}
