class Solution {
    public int heightChecker(int[] heights) {
        int c = 0;
        int n = heights.length;
        int co[] = Arrays.copyOf(heights, n);
        Arrays.sort(co);
        for(int i=0;i<n;i++){
            if(heights[i]!=co[i]){
                c++;
            }
        }
        return c;
    }
}
