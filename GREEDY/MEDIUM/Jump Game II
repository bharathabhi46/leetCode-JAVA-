class Solution {
    public int jump(int[] nums) {
        int n = nums.length;
        int j = 0;
        int l = 0;
        int r = 0;
        while (r < n - 1) {
            int f = 0;
            for (int i = l; i <= r; i++) {
                f = Math.max(f, i + nums[i]);
            }
            j++;
            l = r + 1;
            r = f;
        }
        return j;
    }
}
