class Solution {
    public int eraseOverlapIntervals(int[][] intervals) {
        int n = intervals.length;
        Arrays.sort(intervals, (a,b)->{
            return a[1]-b[1];
        });
        int c = 0;
        int l = Integer.MIN_VALUE;
        for(int i=0;i<n;i++){
            if(intervals[i][0]>=l){
                c++;
                l = intervals[i][1];
            }
        }
        return n - c;
    }
}
