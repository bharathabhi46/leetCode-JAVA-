class Solution {
    public boolean lemonadeChange(int[] bills) {
        int n = bills.length;
        int f = 0;
        int t = 0;
        int tw = 0;
        for(int i=0;i<n;i++){
            if(bills[i] == 5){
                f++;
            }else if(bills[i] == 10){
                t++;
                if(f>=1){
                    f--;
                }else{
                    return false;
                }
            }else{
                tw++;
                if(t>=1 && f>=1){
                    t--;
                    f--;
                }else if(f>=3){
                    f = f-3;
                }else{
                    return false;
                }
            }
        }
        return true;
    }
}
